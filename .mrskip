# .mrskip allows to skip some combinations of host/repo/command
# Format of each line is: "host" "repo" "command"
# Empty fields are ignored but the 2 ':' characters must be there

# u36sd does not bundle medias
u36sd:books:bundle
u36sd:photos:bundle
u36sd:videos:bundle
u36sd:music:bundle

# no one bundles films/download
:films:bundle
:download:bundle
