# .mrskip allows to skip some combinations of host/repo/command
# Format of each line is: "host" "repo" "repo shortname" "action"
# Empty fields match with everything and the 3 ':' characters must be there

# u36sd does not bundle medias
u36sd::books:bundle
u36sd::photos:bundle
u36sd::videos:bundle
u36sd::music:bundle

# no one bundles films/download
::films:bundle
::download:bundle

# rpi does not bundle $HOME/profile
rpi:/home/olivier/.config/vcsh/repo.d/profile.git::bundle
