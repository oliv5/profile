[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/home.git]
clone    = true
checkout = true
gitk     = gitk
pull     = true
push     = git_push backup master
bundle   = git_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle"
#bundle   = git_bundle "/var/backups/${USER}"
gdm      = git difftool -y -t meld --
gc       = git gc
meta     = git_meta
