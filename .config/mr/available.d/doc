[${PRIVATE:-$HOME}/doc]
skip     = [ ! -e "$MR_REPO" ] || ! git --git-dir="$MR_REPO/.git" rev-parse --verify "HEAD" >/dev/null 2>&1
checkout = annex_clone ssh://olivier@oliv5kta.dtdns.net:443/media/wdc/repo/sync/git/doc.git rpi
status   = annex_status
bundle   = git_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle"
sync     = annex_sync
gitk     = gitk
gc       = git gc
