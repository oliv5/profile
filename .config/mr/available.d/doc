[${PRIVATE:-$HOME}/doc]
checkout = git_clone "$(mr_ask "Remote repo to clone")" "$(mr_ask "Remote repo name")";
           annex_init && annex_init_hubic

bundle   = git_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle/public";
           annex_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle/public"

pull     = true
push     = true
sync     = annex_sync

download = annex_copy . --from hubic --auto
upload   = annex_copy . --to hubic --auto

status   = annex_status
diff     = annex_diff
gdm      = git difftool -y -t meld --

gitk     = gitk
gc       = git gc
meta     = git_meta
