[${PRIVATE:-$HOME}/dev]
bundle   = git_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle"

pull     = git_pull rpi master
push     = git_push rpi master
sync     = repo sync

download = annex_copy . --from hubic --auto
upload   = annex_copy . --to hubic --auto

status   = git status -s
diff     = annex_diff
gdm      = git difftool -y -t meld --

gitk     = gitk
gc       = git gc
meta     = git_meta
