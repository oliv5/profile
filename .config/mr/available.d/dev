bundle   = git_bundle "$HOME/repo/sync/git/bundle/dev.bundle.$(uname -n).$(date +%Y%m%d-%H%M%S).git"
gitk     = gitk
pull     = git_pull rpi master
push     = git_push rpi master
gc       = git gc
