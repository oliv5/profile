[${PRIVATE:-$HOME}/sshpack]
skip     = [ ! -d "${PRIVATE:-$HOME}/sshpack" ]
checkout = git_clone ssh://olivier@oliv5kta.dtdns.net:443/home/olivier/git/sshpack.git rpi
bundle   = git_bundle "$HOME/repo/sync/git/bundle/sshpack.bundle.$(uname -n).$(date +%Y%m%d-%H%M%S).git"
sync     = annex_sync
gitk     = gitk
pull     = git_pull rpi master
push     = git_push "rpi backup" master
gc       = git gc
