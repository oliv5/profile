[${PRIVATE:-$HOME}/sshpack]
skip     = [ ! -d "${PRIVATE:-$HOME}/sshpack" ]
checkout = annex_clone ssh://olivier@oliv5kta.dtdns.net:443/home/olivier/git/sshpack.git rpi
bundle   = git_bundle "${PRIVATE:-$HOME}/repo/sync/git/bundle"
sync     = annex_sync
gitk     = gitk
pull     = git_pull rpi master
push     = git_push rpi master
gc       = git gc
